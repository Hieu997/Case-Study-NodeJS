<div class="page-heading">
  <h1>Library Store</h1>
</div>

<form action="/library/library" method="POST" enctype="multipart/form-data">
  <div class="control-group">
    <div class="form-group floating-label-form-group controls">
      <label>Library Name</label>
      <input type="text" class="form-control" id="libraryname" name="libraryname" placeholder="...">
    </div>
  </div>

  <div class="control-group">
    <div class="form-group floating-label-form-group controls">
      <label>Manager Name</label>
      <input type="text" class="form-control" id="managername" name="managername", placeholder="...">
    </div>
  </div>

  <div class="form-group">
    <button type="submit" class="btn btn-primary">Register</button>
  </div>
</form>